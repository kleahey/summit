<div class="container">
  <h1>Staffing for <%= @session.name %></h1>
  <h3>Location: <%= @schedules.first.location.name %></h3></br>
  <table class="table">
    <thead>
      <tr>
        <th>Team Member</th>
        <th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.teammember.name %></td>
        <td><%= schedule.role.name %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
