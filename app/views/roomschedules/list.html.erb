<div class="container">
  <h1>Staffing for <%= @session.name %></h1>
  <table class="table">
    <thead>
      <tr>
        <th>Team Member</th>
        <th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% @roominfo.each do |room| %>
      <tr>
        <td><%= room.teammember.name %></td>
        <td><%= room.role.name %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
